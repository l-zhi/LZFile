{
  "editor.wordWrap": "on",
  "editor.snippetSuggestions": "top",
  "eslint.enable": true,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.formatOnSave": false,
  "eslint.autoFixOnSave": true,
  "files.associations": {
      "*.tpl": "html",
      "*.wxml": "html",
      "*.wxss": "css"
  },
  "emmet.syntaxProfiles": {
      "vue-html": "html",
      "vue": "html"
  },
  "vetur.validation.template": false
}